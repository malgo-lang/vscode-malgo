{
  "name": "vscode-malgo",
  "displayName": "Malgo",
  "description": "Syntax highlighting for Malgo",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/malgo-lang/vscode-malgo.git"
  },
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "malgo",
        "aliases": [
          "Malgo",
          "malgo"
        ],
        "extensions": [
          ".mlg"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "malgo",
        "scopeName": "source.malgo",
        "path": "./syntaxes/malgo.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "js-yaml": "^4.1.0"
  },
  "private": true
}
